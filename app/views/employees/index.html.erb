<v-layout row>
  <v-flex xs12 sm6 offset-sm3>
    <h2>Employees</h2>
    <v-layout row v-for="(error, index) in errors" :key="index">
      <v-flex xs12>
        <vue-alert @alertdismissed="onDismissed(index)" :error="error"></vue-alert>
      </v-flex>
    </v-layout>
  </v-flex>
</v-layout>
<% @employees.each do |employee| %>
    <employee-card card="<%= employee.to_json %>"></employee-card>
<% end %>
<%= form_for(Employee.new) do |f| %>
<% end %>